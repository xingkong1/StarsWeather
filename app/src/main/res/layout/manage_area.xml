    android:orientation="vertical"
    android:id="@+id/manage_area"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.xingkong.starsweather.ManageAreaActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/manage_area_fragment"
            android:name="com.xingkong.starsweather.ManageAreaFragment"/>
    </FrameLayout>

</LinearLayout>
